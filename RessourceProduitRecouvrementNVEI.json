{
    "openapi": "3.0.1",
    "info": {
        "title": "Recouvrement d'assurance de nvei",
        "description": "",
        "termsOfService": "http://swagger.io/terms/",
        "contact": {
            "email": "apiteam@swagger.io"
        },
        "license": {
            "name": "Apache 2.0",
            "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
        }
    },
    "paths": {
        "/nvei/prime": {
            "post": {
                "requestBody": {
                    "description": "Demande d'information",
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/DemandeInformationPrimesNVEI"
                            }
                        }
                    },
                    "required": true
                },
                "tags": [
                    "Recouvrement d'assurance de nvei"
                ],
                "responses": {
                    "default": {
                        "content": {
                            "*/*": {
                                "schema": {
                                    "$ref": "#/components/schemas/Prime"
                                }
                            }
                        },
                        "description": "default response"
                    }
                },
                "operationId": "preSouscription",
                "description": "Demande d'informations sur le recouvrement ou le reversement d'une prime"
            }
        },
        "/nvei/impaye": {
            "post": {
                "requestBody": {
                    "content": {
                        "*/*": {
                            "schema": {
                                "$ref": "#/components/schemas/DemandeInformationPrimesNVEI"
                            },
                            "examples": {
                                "exemple1": {
                                    "value": {
                                        "contrat": {
                                            "numero": "some text",
                                            "client": {
                                                "nom": "some text",
                                                "prenom": "some text",
                                                "dateDeNaissance": "2018-02-10T09:30Z"
                                            },
                                            "objet": {
                                                "type": "Giropode",
                                                "marque": "some text",
                                                "modele": "some text"
                                            }
                                        },
                                        "sens": "some text",
                                        "evenement": "some text",
                                        "montant": 73.06,
                                        "periode": "Mensuel"
                                    }
                                }
                            }
                        }
                    }
                },
                "tags": [
                    "Recouvrement d'assurance de nvei"
                ],
                "responses": {
                    "default": {
                        "content": {
                            "*/*": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/Impaye"
                                    }
                                }
                            }
                        },
                        "description": "default response"
                    }
                },
                "operationId": "impaye",
                "description": "Demande d'informations sur les impay√©s"
            }
        }
    },
    "components": {
        "schemas": {
            "Prime": {
                "type": "object",
                "properties": {
                    "dateEmmision": {
                        "format": "date-time",
                        "type": "string"
                    },
                    "dateCreation": {
                        "format": "date-time",
                        "type": "string"
                    },
                    "montant": {
                        "format": "float",
                        "type": "number"
                    }
                }
            },
            "Client": {
                "type": "object",
                "properties": {
                    "nom": {
                        "type": "string"
                    },
                    "prenom": {
                        "type": "string"
                    },
                    "dateDeNaissance": {
                        "format": "date-time",
                        "type": "string"
                    }
                }
            },
            "ContratNVEI": {
                "type": "object",
                "properties": {
                    "numero": {
                        "type": "string"
                    },
                    "client": {
                        "$ref": "#/components/schemas/Client"
                    },
                    "objet": {
                        "$ref": "#/components/schemas/NVEI"
                    }
                }
            },
            "DemandeInformationPrimesNVEI": {
                "type": "object",
                "properties": {
                    "contrat": {
                        "$ref": "#/components/schemas/ContratNVEI"
                    },
                    "sens": {
                        "type": "string"
                    },
                    "evenement": {
                        "type": "string"
                    },
                    "montant": {
                        "format": "float",
                        "type": "number"
                    },
                    "periode": {
                        "enum": [
                            "Annuel",
                            "Mensuel",
                            "Trimestriel"
                        ],
                        "type": "string"
                    }
                }
            },
            "NVEI": {
                "type": "object",
                "properties": {
                    "type": {
                        "enum": [
                            "Giropode",
                            "Overboard",
                            "Trotinette"
                        ],
                        "type": "string"
                    },
                    "marque": {
                        "type": "string"
                    },
                    "modele": {
                        "type": "string"
                    }
                }
            },
            "Impaye": {
                "type": "object",
                "properties": {
                    "dateDemande": {
                        "format": "date-time",
                        "type": "string"
                    },
                    "montant": {
                        "format": "float",
                        "type": "number"
                    }
                }
            }
        }
    }
}
