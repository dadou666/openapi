{
  "openapi" : "3.0.1",
  "info" : {
    "title" : "Gestion production d'assurance de nvei",
    "description" : "",
    "termsOfService" : "http://swagger.io/terms/",
    "contact" : {
      "email" : "apiteam@swagger.io"
    },
    "license" : {
      "name" : "Apache 2.0",
      "url" : "http://www.apache.org/licenses/LICENSE-2.0.html"
    }
  },
  "paths" : {
    "/nvei/avenant" : {
      "post" : {
        "tags" : [ "Gestion production d'assurance de nvei" ],
        "description" : "Demande des modifications sur les adhésions",
        "operationId" : "avenant",
        "requestBody" : {
          "content" : {
            "*/*" : {
              "schema" : {
                "$ref" : "#/components/schemas/DemandeInformationAvenantVelo"
              }
            }
          }
        },
        "responses" : {
          "default" : {
            "description" : "default response",
            "content" : {
              "*/*" : {
                "schema" : {
                  "$ref" : "#/components/schemas/AvenantVelo"
                }
              }
            }
          }
        }
      }
    },
    "/nvei/emission" : {
      "post" : {
        "tags" : [ "Gestion production d'assurance de nvei" ],
        "description" : " Demande d'informations sur les primes",
        "operationId" : "emission",
        "requestBody" : {
          "content" : {
            "*/*" : {
              "schema" : {
                "$ref" : "#/components/schemas/DemandeInformationPrimesVelo"
              }
            }
          }
        },
        "responses" : {
          "default" : {
            "description" : "default response",
            "content" : {
              "*/*" : {
                "schema" : {
                  "$ref" : "#/components/schemas/EmissionVelo"
                }
              }
            }
          }
        }
      }
    },
    "/nvei/annulation" : {
      "post" : {
        "tags" : [ "Gestion production d'assurance de nvei" ],
        "description" : "Demande d'informations sur les annulations contrats",
        "operationId" : "annulation",
        "requestBody" : {
          "content" : {
            "*/*" : {
              "schema" : {
                "$ref" : "#/components/schemas/DemandeInformationVelo"
              }
            }
          }
        },
        "responses" : {
          "default" : {
            "description" : "default response",
            "content" : {
              "*/*" : {
                "schema" : {
                  "$ref" : "#/components/schemas/AnnulationVelo"
                }
              }
            }
          }
        }
      }
    },
    "/nvei/resiliation" : {
      "post" : {
        "tags" : [ "Gestion production d'assurance de nvei" ],
        "description" : "Demande d'informations sur  les résiliations contrats",
        "operationId" : "resiliation",
        "requestBody" : {
          "content" : {
            "*/*" : {
              "schema" : {
                "$ref" : "#/components/schemas/DemandeInformationVelo"
              }
            }
          }
        },
        "responses" : {
          "default" : {
            "description" : "default response",
            "content" : {
              "*/*" : {
                "schema" : {
                  "$ref" : "#/components/schemas/ResiliationVelo"
                }
              }
            }
          }
        }
      }
    }
  },
  "components" : {
    "schemas" : {
      "AvenantVelo" : {
        "type" : "object",
        "properties" : {
          "dateAvenant" : {
            "type" : "string",
            "format" : "date-time"
          },
          "contratAvant" : {
            "$ref" : "#/components/schemas/ContratVelo"
          },
          "contratApres" : {
            "$ref" : "#/components/schemas/ContratVelo"
          }
        }
      },
      "Client" : {
        "type" : "object",
        "properties" : {
          "nom" : {
            "type" : "string"
          },
          "prenom" : {
            "type" : "string"
          },
          "dateDeNaissance" : {
            "type" : "string",
            "format" : "date-time"
          }
        }
      },
      "ContratVelo" : {
        "type" : "object",
        "properties" : {
          "numero" : {
            "type" : "string"
          },
          "client" : {
            "$ref" : "#/components/schemas/Client"
          },
          "objet" : {
            "$ref" : "#/components/schemas/Velo"
          }
        }
      },
      "Velo" : {
        "type" : "object",
        "properties" : {
          "marque" : {
            "type" : "string"
          },
          "modele" : {
            "type" : "string"
          },
          "numeroDeCadre" : {
            "type" : "string"
          },
          "numeroAntiVol" : {
            "type" : "string"
          }
        }
      },
      "DemandeInformationAvenantVelo" : {
        "type" : "object",
        "properties" : {
          "contratAncien" : {
            "$ref" : "#/components/schemas/ContratVelo"
          },
          "contratNouveau" : {
            "$ref" : "#/components/schemas/ContratVelo"
          },
          "dateEffet" : {
            "type" : "string",
            "format" : "date-time"
          },
          "dateCreation" : {
            "type" : "string",
            "format" : "date-time"
          }
        }
      },
      "EmissionVelo" : {
        "type" : "object",
        "properties" : {
          "contrat" : {
            "$ref" : "#/components/schemas/ContratVelo"
          },
          "primes" : {
            "type" : "array",
            "items" : {
              "$ref" : "#/components/schemas/Prime"
            }
          }
        }
      },
      "Prime" : {
        "type" : "object",
        "properties" : {
          "dateEmmision" : {
            "type" : "string",
            "format" : "date-time"
          },
          "dateCreation" : {
            "type" : "string",
            "format" : "date-time"
          },
          "montant" : {
            "type" : "number",
            "format" : "float"
          }
        }
      },
      "DemandeInformationPrimesVelo" : {
        "type" : "object",
        "properties" : {
          "contrat" : {
            "$ref" : "#/components/schemas/ContratVelo"
          },
          "sens" : {
            "type" : "string"
          },
          "evenement" : {
            "type" : "string"
          },
          "montant" : {
            "type" : "number",
            "format" : "float"
          },
          "periode" : {
            "type" : "string",
            "enum" : [ "Annuel", "Mensuel", "Trimestriel" ]
          }
        }
      },
      "AnnulationVelo" : {
        "type" : "object",
        "properties" : {
          "contrat" : {
            "$ref" : "#/components/schemas/ContratVelo"
          },
          "dateAnnulation" : {
            "type" : "string",
            "format" : "date-time"
          },
          "dateCreation" : {
            "type" : "string",
            "format" : "date-time"
          }
        }
      },
      "DemandeInformationVelo" : {
        "type" : "object",
        "properties" : {
          "contrat" : {
            "$ref" : "#/components/schemas/ContratVelo"
          },
          "dateEffet" : {
            "type" : "string",
            "format" : "date-time"
          },
          "dateCreation" : {
            "type" : "string",
            "format" : "date-time"
          }
        }
      },
      "ResiliationVelo" : {
        "type" : "object",
        "properties" : {
          "contrat" : {
            "$ref" : "#/components/schemas/ContratVelo"
          },
          "dateResiliation" : {
            "type" : "string",
            "format" : "date-time"
          },
          "dateCreation" : {
            "type" : "string",
            "format" : "date-time"
          }
        }
      }
    }
  }
}